.slider {
    position: relative;
    padding-bottom: 70px;
    max-width: 1920px;
    margin-left: auto;
    margin-right: auto;

    @media (--md-viewport) {
        padding-bottom: 80px;
    }
}

.end-padding-slider {
    max-width: 1920px;
    margin: 45px auto 0;
}

.tiny-slider {
    display: flex;
    backface-visibility: hidden;
    -webkit-font-smoothing: subpixel-antialiased;

    &__item {
        margin-left: 10px !important;
        margin-right: 10px !important;
        padding-bottom: 80px;
        position: relative;

        &.-tree1 {
            background: url('data:image/png;base64,') center 80% no-repeat;
            background-size: 201px auto;
        }

        &.-tree2 {
            background: url('data:image/png;base64,') center 80% no-repeat;
            background-size: 234px auto;
        }

        &.-tree3 {
            background: url('data:image/png;base64,') center 80% no-repeat;
            background-size: 170px auto;
        }

        .inner {
            background: var(--white);
            position: absolute;
            top: 0;
            left: 0;
            right: 0;
            height: 100%;
            z-index: -1;
        }


        img {
            margin-bottom: 0;
            width: 100%;
        }

        p {
            margin-bottom: 20px;
        }

        &__content {
            padding: 30px 20px;

            h3 {
                margin-bottom: 10px;
            }
        }

        .button {
            min-width: 200px;
            position: absolute;
            bottom: 28px;
            left: 50%;
            margin-left: -100px;
        }
    }

    .room-price {
        color: var(--gray);
        font-size: 25px;
        margin-bottom: 40px;

        strong {
            font-size: 30px;
            font-weight: 600;

            @media only screen and (min-width: 1340px) {
                font-size: 34px;
            }
        }
    }
}

.tns-controls {
    position: absolute;
    bottom: 0;
    left: 0;
    right: 0;
    text-align: center;
}

.tns-controls [aria-controls] {
    text-indent: -9999px;
    background: none;
    width: 0;
    height: 0;
    border-style: solid;
    position: relative;
    outline: none;
    transition: all .3s;

    &[data-controls = 'prev'] {
        border-width: 15px 38px 15px 0;
        border-color: transparent var(--gray-dark) transparent transparent;
        margin-right: 25px;

        &::after {
            content: '';
            position: absolute;
            right: -38px;
            top: -14px;
            width: 0;
            height: 0;
            border-style: solid;
            border-width: 15px 10px 15px 0;
            border-color: transparent var(--gray-light) transparent transparent;
        }

        @media (--md-viewport) {
            &:hover {
                border-color: transparent var(--red) transparent transparent;
            }
        }
    }

    &[data-controls = 'next'] {
        border-width: 15px 0 15px 38px;
        border-color: transparent transparent transparent var(--gray-dark);
        margin-left: 25px;

        &::after {
            content: '';
            position: absolute;
            left: -38px;
            top: -14px;
            width: 0;
            height: 0;
            border-style: solid;
            border-width: 15px 0 15px 10px;
            border-color: transparent transparent transparent var(--gray-light);
        }

        @media (--md-viewport) {
            &:hover {
                border-color: transparent transparent transparent var(--red);
            }
        }
    }
}

.gallery-slider {
    img {
        margin: 0;
    }

    &__img {
        display: block;
    }
}
