.circle-logo {
    width: 168px;
    margin: 0 auto 50px;
    display: block;

    @media (--md-viewport) {
        margin-bottom: 115px;
    }
}

.map-iframe {
    margin-top: 0;
    border: none;
}

.contacts-info-container {
    background: url('data:image/jpeg;base64,') 100% bottom no-repeat;
    background-size: 197px auto;
}

.contacts-info {
    margin: 30px 0 50px;

    @media (--md-viewport) {
        margin-bottom: 100px;
    }

    &__item {
        font-size: 18px;
        position: relative;
        padding-left: 44px;
        margin-bottom: 25px;

        .icon {
            position: absolute;
            left: 0;
            top: -2px;
        }

    }
}
